# Configuração RabbitMQ
# Formato: amqp://[username]:[password]@[hostname]:[port]
RABBITMQ=amqp://admin:admin@rabbitmq:5672

# Número máximo de tentativas de reprocessamento
MAX_RETRIES=5

# Porta para o servidor de health check
HEALTH_PORT=3000

# Ambiente (development, production, test)
NODE_ENV=production

# Configurações adicionais para serviços de notificação
# FCM_API_KEY=sua_chave_fcm_aqui
# FCM_SENDER_ID=seu_sender_id_aqui

# Configurações de logging
LOG_LEVEL=info


FCM_API_KEY=your_fcm_api_key
FCM_PROJECT_ID=your_project_id
FCM_CLIENT_EMAIL=your_client_email
FCM_PRIVATE_KEY="your_private_key_with_quotes"